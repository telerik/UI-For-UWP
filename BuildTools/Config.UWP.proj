<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">

  <PropertyGroup>
    <DeployDirectory>$(MSBuildProjectDirectory)\..</DeployDirectory>
    <BinariesTargetDirectory>Binaries</BinariesTargetDirectory>
    <BinariesOutDir Condition=" '$(BinariesOutDir)' == '' ">..\..\..\Binaries</BinariesOutDir>
    <PowerShellExe Condition=" '$(PowerShellExe)'=='' ">%WINDIR%\System32\WindowsPowerShell\v1.0\powershell.exe</PowerShellExe>
    <Nuget32ToolPath>"$(MSBuildProjectDirectory)\nuget.exe"</Nuget32ToolPath>
    <ControlsFolder>$(MsBuildProjectDirectory)\..\Controls</ControlsFolder>
    <UWPControlsSolutionPath>$(ControlsFolder)\UWPControls.sln</UWPControlsSolutionPath>
    <UWPMapSolutionPath>$(ControlsFolder)\UWPMap.sln</UWPMapSolutionPath>
    <Configuration>Release</Configuration>
    <AnyCPUPlatform>Any CPU</AnyCPUPlatform>
    <X86Platform>x86</X86Platform>
    <X64Platform>x64</X64Platform>
    <ArmPlatform>ARM</ArmPlatform>
    <Arm64Platform>ARM64</Arm64Platform>
	<MSBuild16ExePath>"C:\Program Files (x86)\Microsoft Visual Studio\2019\Enterprise\MSBuild\Current\Bin"</MSBuild16ExePath>
  </PropertyGroup>

</Project>
