<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:telerik="using:Telerik.UI.Xaml.Controls">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ms-appx:///Telerik.UI.Xaml.Primitives.UWP/Themes/ThemeColorsDark.xaml"/>
        <ResourceDictionary>
            <telerik:UserThemeResources x:Key="ResourceLoaderInitializer"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="{CustomResource DarkResourcesPath}"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>

    <!--#region SegmentedControl Brushes-->

    <Color x:Key="TelerikSegmentedControlDisabledColor">#FF333333</Color>

    <SolidColorBrush x:Key="TelerikCalendarBackgroundBrush" Color="#FF1D1D1D"/>
    <SolidColorBrush x:Key="TelerikSegmentedControlBackgroundThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikSegmentedControlBorderThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikSegmentedControlSeparatorThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />

    <SolidColorBrush x:Key="TelerikSegmentedControlDisabledBackgroundThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikSegmentedControlDisabledBorderThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />
    <SolidColorBrush x:Key="TelerikSegmentedControlDisabledSeparatorThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />

    <!--segment normal-->

    <SolidColorBrush x:Key="TelerikSegmentBackgroundThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikSegmentContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" Opacity="0.6" />
    <SolidColorBrush x:Key="TelerikSegmentBorderThemeBrush" Color="Transparent" />

    <!--segment pointer over-->

    <SolidColorBrush x:Key="TelerikSegmentPointerOverBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" Opacity="0.6" />
    <SolidColorBrush x:Key="TelerikSegmentPointerOverContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentPointerOverBorderThemeBrush" Color="Transparent" />

    <!--segment pressed-->

    <SolidColorBrush x:Key="TelerikSegmentPressedBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikSegmentPressedContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentPressedBorderThemeBrush" Color="Transparent" />

    <!--segment disabled-->

    <SolidColorBrush x:Key="TelerikSegmentDisabledBackgroundThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikSegmentDisabledContentForegroundThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />
    <SolidColorBrush x:Key="TelerikSegmentDisabledBorderThemeBrush" Color="Transparent" />

    <!--segment selected-->

    <SolidColorBrush x:Key="TelerikSegmentSelectedBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedBorderThemeBrush" Color="Transparent" />

    <!--segment selected pointer over-->

    <SolidColorBrush x:Key="TelerikSegmentSelectedPointerOverBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" Opacity="0.8" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedPointerOverContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedPointerOverBorderThemeBrush" Color="Transparent" />

    <!--segment selected pressed-->

    <SolidColorBrush x:Key="TelerikSegmentSelectedPressedBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedPressedContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedPressedBorderThemeBrush" Color="Transparent" />

    <!--segment selected disabled-->

    <SolidColorBrush x:Key="TelerikSegmentSelectedDisabledBackgroundThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedDisabledContentForegroundThemeBrush" Color="#FF545454" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedDisabledBorderThemeBrush" Color="Transparent" />

    <!--#endregion-->

    <!--#region AutoCompleteBox Brushes-->

    <!--Inline-->

    <SolidColorBrush x:Key="TelerikAutoCompleteBoxForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxBackgroundBrush" Color="#CCFFFFFF" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxBorderBrush" Color="#CCFFFFFF" />

    <SolidColorBrush x:Key="TelerikAutoCompleteBoxHeaderForegroundBrush" Color="{ThemeResource TelerikSecondaryForegroundColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxWatermarkForegroundBrush" Color="{ThemeResource TelerikWatermarkForegroundColor}" />

    <!--PointerOver-->
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxPointerOverBackgroundBrush" Color="#DEFFFFFF" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxPointerOverBorderBrush" Color="#DEFFFFFF" />

    <!--Focused-->
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxFocusedForegroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxFocusedBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxFocusedBorderBrush" Color="{ThemeResource TelerikWhiteColor}" />

    <!--Disabled-->
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxDisabledForegroundBrush" Color="{ThemeResource TelerikDisabledColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxDisabledBackgroundBrush" Color="#00FFFFFF" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxDisabledBorderBrush" Color="{ThemeResource TelerikDisabledColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxWatermarkDisabledForegroundBrush" Color="{ThemeResource TelerikWatermarkDisabledForegroundColor}" />

    <!--Selector-->
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorBorderBrush" Color="White" />

    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorItemBorderBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorSelectedItemBackgroundBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorSelectedItemBorderBrush" Color="{ThemeResource TelerikSelectedColor}" />

    <!--#endregion-->

    <!--#region DateTimePicker Brushes-->

    <!--Header-->
    <SolidColorBrush x:Key="TelerikDateTimePickerHeaderForegroundBrush" Color="{ThemeResource TelerikSecondaryForegroundColor}" />

    <!--Rest state-->
    <SolidColorBrush x:Key="TelerikDateTimePickerBackgroundBrush" Color="#CCFFFFFF" />
    <SolidColorBrush x:Key="TelerikDateTimePickerForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerBorderBrush" Color="#CCFFFFFF" />
    <SolidColorBrush x:Key="TelerikDateTimePickerArrowForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />

    <!--Hover state-->
    <SolidColorBrush x:Key="TelerikDateTimePickerPointerOverBackgroundBrush" Color="#DEFFFFFF" />
    <SolidColorBrush x:Key="TelerikDateTimePickerPointerOverBorderBrush" Color="#DEFFFFFF" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectedPointerOverBackgroundBrush" Color="{ThemeResource TelerikSelectedColor}" />

    <!--Focused state-->
    <SolidColorBrush x:Key="TelerikDateTimePickerFocusedBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerFocusedBorderBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerFocusedForegroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectedBackgroundBrush" Color="{ThemeResource TelerikSelectedColor}" />

    <!--Pressed state-->
    <SolidColorBrush x:Key="TelerikDateTimePickerPressedBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerPressedBorderBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerPressedForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerArrowPressedForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerPressedHighlightBrush" Color="#FFD3D3D3" />

    <!--Disabled state-->
    <SolidColorBrush x:Key="TelerikDateTimePickerDisabledBackgroundBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikDateTimePickerDisabledBorderBrush" Color="{ThemeResource TelerikDisabledColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerDisabledForegroundBrush" Color="{ThemeResource TelerikDisabledColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerArrowDisabledForegroundBrush" Color="{ThemeResource TelerikDisabledColor}" />

    <!--Selector-->
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorBorderBrush" Color="#FF151515" />

    <!--SelectorItem Rest State-->
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemBackgroundBrush" Color="#B2B6B6B6" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemForegroundBrush" Color="#FF212121" />

    <!--SelectorItem Select State-->
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemSelectedBackgroundBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemSelectedForegroundBrush" Color="{ThemeResource TelerikWhiteColor}" />

    <!--SelectorItem Disabled State-->
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemDisabledBackgroundBrush" Color="#66CACACA" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemDisabledForegroundBrush" Color="#7C212121" />

    <!--SelectorIconButton States-->
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorButtonBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorButtonForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorButtonBorderBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorButtonPointerOverBackgroundBrush" Color="#75B6B6B6" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorButtonPressedBackgroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorButtonPressedForegroundBrush" Color="{ThemeResource TelerikWhiteColor}" />

    <!--#endregion-->

    <!--#region NumericBox Brushes-->

    <!--Normal State-->
    <SolidColorBrush x:Key="TelerikNumericBoxBackgroundBrush" Color="#CCFFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxBorderBrush" Color="#CCFFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxHeaderForegroundBrush" Color="{ThemeResource TelerikSecondaryForegroundColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxWatermarkForegroundBrush" Color="{ThemeResource TelerikWatermarkForegroundColor}" />

    <!--PointerOver State-->
    <SolidColorBrush x:Key="TelerikNumericBoxPointerOverBackgroundBrush" Color="#DEFFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxPointerOverBorderBrush" Color="#DEFFFFFF" />

    <!--Focused State-->
    <SolidColorBrush x:Key="TelerikNumericBoxFocusedBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxFocusedBorderBrush" Color="{ThemeResource TelerikWhiteColor}" />

    <!--Disabled State-->
    <SolidColorBrush x:Key="TelerikNumericBoxDisabledBackgroundBrush" Color="#00FFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxDisabledForegroundBrush" Color="{ThemeResource TelerikWatermarkForegroundColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxDisabledBorderBrush" Color="#66FFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxWatermarkDisabledForegroundBrush" Color="{ThemeResource TelerikWatermarkDisabledForegroundColor}" />

    <!--Spin Button-->
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonBackgroundBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonForegroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonPointerOverBackgroundBrush" Color="#FF217DC9" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonPressedBackgroundBrush" Color="{ThemeResource TelerikForegroundColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonPressedForegroundBrush" Color="#FF000000" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonDisabledBackgroundBrush" Color="#00FFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonDisabledForegroundBrush" Color="{ThemeResource TelerikDisabledColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonDisabledBorderBrush" Color="#66FFFFFF" />

    <!--Delete Button-->
    <SolidColorBrush x:Key="TelerikNumericBoxDeleteButtonForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxDeleteButtonPointerOverBackgroundBrush" Color="#C4CDCDCD" />
    <SolidColorBrush x:Key="TelerikNumericBoxDeleteButtonPressedBackgroundBrush" Color="{ThemeResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxDeleteButtonPressedForegroundBrush" Color="{ThemeResource TelerikWhiteColor}" />

    <!--#endregion-->

    <!--#region Calendar Brushes-->

    <!--GridLines-->
    <SolidColorBrush x:Key="TelerikCalendarGridLinesBrush" Color="#FF696969" />
    <SolidColorBrush x:Key="TelerikSelectedBrush" Color="#FF006AC1" />

    <!--DayNames-->
    <SolidColorBrush x:Key="TelerikCalendarDayNamesForegroundBrush" Color="#FFCCCCCC" />

    <!--CellStyles-->
    <SolidColorBrush x:Key="TelerikCalendarNormalCellForegroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikCalendarAnotherViewCellForegroundBrush" Color="#FF767676" />

    <SolidColorBrush x:Key="TelerikCalendarBlackoutCellForegroundBrush" Color="#FF767676" />
    <SolidColorBrush x:Key="TelerikCalendarBlackoutCellBackgroundBrush" Color="#FF343434" />

    <SolidColorBrush x:Key="TelerikCalendarSelectedCellForegroundBrush" Color="#FF3399FF" />
    <SolidColorBrush x:Key="TelerikCalendarSelectedCellBackgroundBrush" Color="#333399FF" />
    <SolidColorBrush x:Key="TelerikCalendarSelectedCellBorderBrush" Color="#FF3399FF" />

    <SolidColorBrush x:Key="TelerikCalendarHighlightedCellBorderBrush" Color="{ThemeResource TelerikWhiteColor}" />

    <!--HoldClueControl-->
    <SolidColorBrush x:Key="TelerikCalendarHoldClueControlBackgroundBrush" Color="#FF3399FF" />

    <!--NavigationControl-->
    <SolidColorBrush x:Key="TelerikCalendarNavigationControlBackgroundBrush" Color="#FF373737" />

    <SolidColorBrush x:Key="TelerikCalendarPreviousNextButtonForegroundBrush" Color="#FFCCCCCC" />
    <SolidColorBrush x:Key="TelerikCalendarPreviousNextButtonDisabledForegroundBrush" Color="#FF767676" />
    <SolidColorBrush x:Key="TelerikCalendarPreviousNextButtonPointerOverForegroundBrush" Color="#FF3399FF" />
    <SolidColorBrush x:Key="TelerikCalendarPreviousNextButtonPointerPressedForegroundBrush" Color="#FF3399FF" />

    <SolidColorBrush x:Key="TelerikCalendarHeaderButtonForegroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikCalendarHeaderButtonDisabledForegroundBrush" Color="#FF767676" />
    <SolidColorBrush x:Key="TelerikCalendarHeaderButtonPointerPressedForegroundBrush" Color="#FF3399FF" />
    <SolidColorBrush x:Key="TelerikCalendarHeaderButtonPointerOverForegroundBrush" Color="#FF3399FF" />

    <!--PointerOver-->
    <SolidColorBrush x:Key="TelerikCalendarPointerOverBackgroundBrush" Color="#193399FF" />
    <SolidColorBrush x:Key="TelerikCalendarPointerOverBorderBrush" Color="#FF3399FF" />

    <SolidColorBrush x:Key="TelerikCalendarCurrentCellBorderBrush" Color="#FF0066CC" />

    <!--SpecialSlots-->
    <SolidColorBrush x:Key="TelerikSpecialSlotBackground" Color="#919191" Opacity="0.2"/>
    <SolidColorBrush x:Key="TelerikSpecialSlotBackgroundReadOnly" Color="#919191" Opacity="0.35"/>

    <!--Today MultiDayView-->
    <SolidColorBrush x:Key="TelerikTodayMultiDayViewBackground" Color="#898989"/>

    <!--#endregion-->
</ResourceDictionary>
