<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:telerik="using:Telerik.UI.Xaml.Controls">

    <SolidColorBrush x:Key="TelerikMapBackgroundBrush" Color="#FF373737"/>
    <SolidColorBrush x:Key="TelerikMapBorderBrush" Color="Transparent"/>

    <Color x:Key="TelerikMapShapeFillColor">#FFCCCCCC</Color>
    <Color x:Key="TelerikMapShapeStrokeColor">#FF373737</Color>
    <Color x:Key="TelerikMapShapeForegroundColor">#FFFFFFFF</Color>

    <Color x:Key="TelerikMapShapePointerOverFillColor">#FF2086D9</Color>
    <Color x:Key="TelerikMapShapePointerOverStrokeColor">#FF373737</Color>
    <Color x:Key="TelerikMapShapePointerOverForegroundColor">#FFFFFFFF</Color>

    <Color x:Key="TelerikMapShapeSelectedFillColor">#FF006AC1</Color>
    <Color x:Key="TelerikMapShapeSelectedStrokeColor">#FF373737</Color>
    <Color x:Key="TelerikMapShapeSelectedForegroundColor">#FFFFFFFF</Color>

    <SolidColorBrush x:Key="TelerikMapToolTipForeground" Color="#FFDEDEDE" />
    <SolidColorBrush x:Key="TelerikMapToolTipBackground" Color="#FF262626" />

    <telerik:UserThemeResources x:Key="ResourceLoaderInitializer"/>

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="{CustomResource DarkResourcesPath}"/>
    </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>
